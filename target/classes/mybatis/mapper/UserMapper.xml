<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.system.dao.UserMapper">

	<!-- 分页查询所有的用户 -->
	<select id="queryAllUserInfo" resultType="User">
	    SELECT
	        *
	    FROM
	        user
	    ORDER BY
	        id DESC
	</select>
	
</mapper>





